{
  "manifest_version": 3,
  "name": "Filter XOXO_TwinstpVer",
  "description": "A Chrome extension that filters xoxo posts.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.xoxohth.com/*",
        "https://xoxohth.com/*",
        "http://xoxohth.com/*",
        "http://www.xoxohth.com/*",
        "https://*.xoxohth.com/*",
        "http://*.xoxohth.com/*",
        "https://www.autoadmit.com/*",
        "https://autoadmit.com/*",
        "http://autoadmit.com/*",
        "http://www.autoadmit.com/*"
      ],
      "js": ["crypto-js.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.json", "crypto-js.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}